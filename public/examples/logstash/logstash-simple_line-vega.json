{
  "$schema": "https://vega.github.io/schema/vega/v3.0.json",
  "data": [
    {
      "name": "table",
      "url": {
        "index": "logstash-*",
        "body": {
          "query": {
            "range": {"@timestamp": {"%timefilter%": true}}
          },
          "size": 0,
          "aggs": {
            "hist": {
              "date_histogram": {
                "field": "@timestamp",
                "interval": "2h",
                "time_zone": "America/New_York",
                "min_doc_count": 1
              }
            }
          }
        }
      },
      "format": {
        "type": "json",
        "property": "aggregations.hist.buckets"
      }
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "time",
      "domain": {"data": "table", "field": "key"},
      "range": "width"
    },
    {
      "name": "yscale",
      "domain": {"data": "table", "field": "doc_count"},
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {"orient": "bottom", "scale": "xscale", "tickCount": 7},
    {"orient": "left", "scale": "yscale"}
  ],
  "marks": [
    {
      "name": "line",
      "type": "line",
      "from": {"data": "table"},
      "encode": {
        "enter": {
          "x": {"scale": "xscale", "field": "key"},
          "y": {"scale": "yscale", "field": "doc_count"},
          "stroke": {"value": "#35B"},
          "strokeWidth": {"value": 3}
        }
      }
    }
  ]
}
